{
  "version": 2,
  "builds": [
    {
      "src": "backend",    
      "use": "@vercel/node"   
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",  
      "dest": "http://localhost:5000/$1", 
      "methods": ["GET", "POST", "PUT", "DELETE"]  
    },
    {
      "src": "/(.*)",  
      "dest": "frontend/$1"  
    }
  ]
}
