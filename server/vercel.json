{
  "version":2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    },
    {
      "src": "./api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "api/items",
      "dest": "./src/routers/item.js"
    },
    {
      "src": "/api/users",
      "dest": "./src/routers/user.js"
    },
    {
      "src": "/api/socialMedias",
      "dest": "./src/routers/socialMedia.js"
    },
    {
      "src": "/api/comments",
      "dest": "./src/routers/Comment.js"
    },
    {
      "src": "/api/confirmcomments",
      "dest": "./src/routers/ConfirmComment.js"
    },
    {
      "src": "/api/collections",
      "dest": "./src/routers/Collection.js"
    },
    {
      "src": "/api/imagegallery",
      "dest": "./src/routers/backGroundGallery.js"
    }
  ]
  
}